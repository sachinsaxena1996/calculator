<h1>Evaluation Results</h1>
<table border="2">
  <thead>
    <tr>
      <th>Expression</th>
      <th>Evaluated Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @calculations.each do |calculation| %>
      <tr>
        <td><%= calculation.expression %></td>
        <td><%= calculation.evaluation %></td>        
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Calculation', new_calculation_path %>